{
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    
    "prettier-plugin-tailwindcss"
  ],
  "semi": true,
  "singleQuote": false,
  "printWidth": 1000,
  "trailingComma": "es5",
  "importOrder": ["^react", "^@?\\w", "^@/(.*)", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
